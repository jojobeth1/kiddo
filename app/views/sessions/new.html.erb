<div class="container">
  <h2>Log In</h2>
  <%= form_for @user, url:"/sessions", method: "post" do |f| %>
  <div class="row">
    <div class="col">
      <label>Email</label>
      <%=f.email_field :email, class:"form-content"%>
    </div>
    <div class="col">
      <label>Password</label>
      <%=f.password_field :password_digest, class: "form-content"%>
    </div>
    <div>
      <%= f.submit "Log In", class: "btn" %>
    </div>
  </div>
  <% end %>
</div>
